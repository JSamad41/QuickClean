@model QuickClean.Models.PropertyContent

@{
    ViewBag.Title = string.Concat("Transaction for: ");
}
<h2>Complete Transaction</h2>

<div id="user-profile-data">
    @using (Html.BeginForm(FormMethod.Post))
    {

        <div class="section">

        </div>

        <div class="section">
            <div class="item">
                <div style="font-weight:bold">@Html.Label(Model.Property.Compensation, "Grand Total")</div>
                <div>$ @Html.Label(Model.Property.Compensation, Model.Property.Compensation, new { @size = "7", @id = "compensation", @readonly = "readonly" })</div>
            </div>
        </div>


        //variable that holds the grand total is         Model.Property.Compensation


        <div class="section">
            
            <button class="button-cancel" id="btnClose" name="btnSubmit" value="close"><i class='fa fa-times'></i> Close</button>

        </div>

    }
</div>



